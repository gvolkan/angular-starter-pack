{
  "name": "angular-starter-pack",
  "version": "0.1.0",
  "description": "Angular Starter Pack",
  "keywords": [
    "angular",
    "angular4",
    "typescript",
    "node.js",
    "express.js",
    "webpack2",
    "gulp",
    "passport",
    "graphQL",
    "mongodb",
    "mongoose",
    "protractor",
    "karma",
    "mocha",
    "chai",
    "sinon",
    "istanbul",
    "angular-material2",
    "flex-layout",
    "scss"
  ],
  "homepage": "https://github.com/ademshadev",
  "bugs": {
    "url": "https://github.com/ademshadev/angular-starter-pack/issues"
  },
  "license": "MIT",
  "author": "ademsha",
  "repository": {
    "type": "git",
    "url": "https://github.com/ademshadev/angular-starter-pack.git"
  },
  "private": true,
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "preinstall": "",
    "postinstall": "./node_modules/protractor/bin/webdriver-manager update",
    "start": ". ./dev.env && cross-env NODE_ENV=development npm-run-all all:deploy_local",
    "all:build_dev": ". ./dev.env && cross-env NODE_ENV=development npm-run-all all:clean all:gulp_build_dev",
    "all:build_dev_deploy_local": ". ./dev.env && cross-env NODE_ENV=development npm-run-all all:clean all:gulp_build_dev all:deploy_local",
    "all:build_prod": ". ./dev.env && cross-env NODE_ENV=production npm-run-all all:clean all:gulp_build_prod",
    "all:clean": "gulp all:clean",
    "all:deploy_local": "gulp all:deploy_local",
    "all:gulp_build_dev": "gulp all:build_dev",
    "all:gulp_build_prod": "gulp all:build_prod",
    "backend:test": ". ./dev.env && cross-env NODE_ENV=testing ./node_modules/.bin/mocha --compilers ts:ts-node/register ./tests/backend/spec/**/*.spec.ts",
    "backend:test-coverage": ". ./dev.env && cross-env NODE_ENV=testing nyc npm run backend:test",
    "backend:test-coverage-open": ". ./dev.env && cross-env NODE_ENV=testing nyc npm run backend:test && gulp backend_tests:open_coverage",
    "backend:e2e": ". ./dev.env && cross-env NODE_ENV=testing gulp backend:e2e",
    "frontend:test": ". ./dev.env && cross-env NODE_ENV=testing ./node_modules/.bin/karma start ./tests/frontend/karma.conf.js",
    "frontend:test-coverage-open": ". ./dev.env && cross-env NODE_ENV=testing npm run frontend:test && gulp frontend_tests:open_coverage"
  },
  "nyc": {
    "include": [
      "src/backend/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "instrument": true,
    "report-dir": "tests/backend/coverage/html"
  },
  "dependencies": {
    "@angular/animations": "~4.2.6",
    "@angular/cdk": "~2.0.0-beta.8",
    "@angular/common": "~4.2.6",
    "@angular/compiler": "~4.2.6",
    "@angular/compiler-cli": "~4.2.6",
    "@angular/core": "~4.2.6",
    "@angular/flex-layout": "~2.0.0-beta.8",
    "@angular/forms": "~4.2.6",
    "@angular/http": "~4.2.6",
    "@angular/material": "~2.0.0-beta.8",
    "@angular/platform-browser": "~4.2.6",
    "@angular/platform-browser-dynamic": "~4.2.6",
    "@angular/router": "~4.2.6",
    "@angular/upgrade": "~4.2.6",
    "@ngx-translate/core": "~6.0.1",
    "@ngx-translate/http-loader": "~0.0.3",
    "agenda": "~0.9.1",
    "bcrypt": "~1.0.2",
    "bluebird": "~3.5.0",
    "body-parser": "~1.17.2",
    "compression": "~1.6.2",
    "connect-flash": "~0.1.1",
    "cookie-parser": "~1.4.3",
    "cookie-session": "~2.0.0-beta.2",
    "core-js": "~2.4.1",
    "cross-env": "^5.0.2",
